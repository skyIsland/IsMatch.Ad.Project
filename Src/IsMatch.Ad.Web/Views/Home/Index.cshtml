@{ 
    Layout = null;
}
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title></title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="viewport" content="initial-scale=1.0,user-scalable=no,maximum-scale=1,width=device-width">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="format-detection" content="telephone=no">

    <style>
        /*.wrap{margin: 0 auto;max-width: 750px;}*/
        /*.wrap img{width: 100%;display: block;}*/
        h3 {
            font-size: 36px;
            font-weight: 400;
        }

        .banner { /*background: url(~/Content/images/index/banner.png);width: 100%;background-size: 100% 100%;height: 630px;*/
            position: relative;
            min-width: 1200px;
        }

        img {
            width: 100%;
            display: block;
        }

        .bannerimg {
            width: :100%;
        }

        .logo, .consulting {
            display: inline-block;
            position: absolute;
            top: 0;
            right: 2%;
        }

        .logo {
            width: 123px;
            height: 47px;
            left: 18.5%;
        }

            .logo img {
                width: 100%;
                display: block;
            }

        .consulting {
            color: #fff;
            padding: 3px 10px;
            border: 1px solid rgba(255,255,255,1);
            border-radius: 20px;
            margin-top: 15px;
            cursor: pointer;
            top: 0;
            right: 18.5%;
        }

        .experience {
            position: absolute;
            background: #fff;
            padding: 7px 40px;
            border-radius: 30px;
            color: #340376;
            bottom: 22%;
            left: 46.2%;
            cursor: pointer;
            font-size: 18px;
            font-weight: bold;
        }

        .live {
            overflow: hidden;
            text-align: center;
            margin-top: 60px;
        }

            .live h3 {
                text-align: center;
                font-size: 36px;
                font-weight: 400;
            }

        .live_left, .live_right {
            display: inline-block; /*float: left;*/
            vertical-align: middle;
        }

        .live_right {
            width: 800px;
            height: 500px;
        }

        .live_left_div_img {
            display: inline-block;
            width: 35px;
        }

        .live_left_div_text {
            display: inline-block;
            margin-top: 5px;
            margin-left: 5px;
        }

            .live_left_div_text p {
                margin-top: 0;
                margin-bottom: 0;
                text-align: left;
            }

                .live_left_div_text p:nth-child(1) {
                    font-size: 17px;
                    font-weight: bold;
                    color: rgba(65,65,65,1);
                }

                .live_left_div_text p:nth-child(2) {
                    margin-top: 5px;
                    color: rgba(102,102,102,1);
                    font-size: 14px;
                }

        .live_left_div_img, .live_left_div_text {
            float: left;
        }

        .live_left_div {
            overflow: hidden;
            margin-top: 15px;
        }

        .live_left_div_button {
            margin-top: 10px;
        }

            .live_left_div_button div {
                background: none;
                border: none;
                float: left;
                cursor: pointer;
            }

                .live_left_div_button div:nth-child(1) {
                    width: 120px;
                    height: 25px;
                }

                    .live_left_div_button div:nth-child(1) img {
                        height: 100%;
                    }

                .live_left_div_button div:nth-child(2) {
                    width: 120px;
                    height: 25px;
                    border: 1px solid rgba(58,97,242,1);
                    border-radius: 20px;
                    font-size: 16px;
                    color: #3A61F2;
                    text-align: center;
                    line-height: 25px;
                    margin-left: 25px;
                }

        .choose {
            text-align: center;
            margin-top: 125px;
        }

            .choose h3 {
                margin-bottom: 35px;
            }

        .chooseimg {
            width: 1200px;
            height: 557px;
            margin: 0 auto;
        }

        .Live_sentiment {
            height: 380px;
            background: rgba(58,97,242,1);
            position: relative;
            margin-top: 60px;
        }

            .Live_sentiment h3 {
                text-align: center;
                padding-top: 4%;
                color: #fff;
            }

        .Live_sentiment_img {
            position: absolute;
            top: -15%;
            width: 1188px;
            height: 450px;
            left: 20%;
        }

        .feedback {
            margin-top: 20%;
        }

            .feedback h3 {
                text-align: center;
            }

        .feedbackimg {
            width: 1185px;
            height: 460px;
            margin: 0 auto;
        }

        .bottomat {
            position: relative;
        }

        .bottomatcode {
            position: absolute;
            width: 180px;
            height: 110px;
            bottom: 120px;
            left: 45%;
        }

        .bottomatimg {
            height: 600px
        }

            .bottomatimg img {
                height: 100%;
            }

        .floor {
            background: #292E33;
            height: 210px;
        }

        .floor_div_img {
            width: 123px;
        }

        .floor_div_img, .floor_div_text {
            display: inline-block;
            color: #fff;
        }

        .floor_div {
            width: 1200px;
            margin: 0 auto;
            border-bottom: 1px solid rgba(255,255,255,1);
            height: 140px;
        }

        .floor_text {
            text-align: center;
            color: #fff;
            margin-top: 25px;
        }

        .floor_div_text {
            margin-left: 10%;
        }

            .floor_div_text div:nth-child(1) {
                margin-bottom: 15px;
            }

            .floor_div_text div:nth-child(2) span:nth-child(2) {
                margin-left: 25px;
            }

        .dialog {
            display: none;
            /* position: fixed; */
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 10;
        }

            .dialog .changePwd {
                /* position: absolute; */
                position: fixed;
                left: 50%;
                top: 15%;
                margin-left: -240px;
                width: 445px;
                height: 484px;
                border-radius: 10px;
                background: url(../../Content/images/index/xak.png);
                background-size: 100% 100%;
            }

        .closeBtn {
            width: 20px;
            height: 20px;
            display: inline-block;
            float: right;
            margin-right: 25px;
            margin-top: 15px;
            cursor: pointer;
            font-size: 20px;
        }

        ul, li {
            padding: 0;
            margin: 0;
            list-style: none
        }

        a {
            text-decoration: none;
        }

        .anchor {
            position: absolute;
            top: 0;
            left: 41%;
            margin-top: 18px;
        }

            .anchor li {
                color: #fff;
                display: inline-block;
                margin-left: 50px;
                font-size: 20px;
                cursor: pointer;
            }

                .anchor li a {
                    color: #fff;
                    font-size: 20px;
                }
    </style>
</head>

<body>
    <div class="wrap">
        <div class="banner">
            <div class="bannerimg"><img src="~/Content/images/index/xiaoai1.png" alt="" /></div>
            <!--<div class="logo"><img src="~/Content/images/index/logo.png" alt="" /></div>-->
            <ul class="anchor">
                <li><a href="#miao1">直播服务</a></li>
                <li><a href="#miao2">视频服务</a></li>
                <li><a href="#miao3">产品优势</a></li>
                <li><a href="#miao5">案例效果</a></li>
                <li><a href="/Home/DxyIndex">下单</a></li>
                <!--<li><a href="http://xjh.ajwlkj12.cn/propaganda.html">代理政策</a></li>-->
            </ul>
            <div class="consulting button">立即咨询</div>
            <!--<div class="experience button">免费体验</div>-->
        </div>

        <div class="live">
            <h3 id="miao1">—— 直播智能增值服务 ——</h3>
            <div class="live_right">
                <img src="~/Content/images/index/Liveillustration.png" alt="" />
            </div>
            <div class="live_left">
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/Popularicn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>直播间人气提升</p>
                        <p>为你的直播批量增加人气，让你直播间不在孤单</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/Scrollingicn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>直播间自定义滚屏发言</p>
                        <p>为直播间形成舆论和人气的聚集，提升直播转化</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/caricn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>快速小黄车购买滚屏</p>
                        <p>小黄车购买提示、加入粉丝团等</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/Videoicn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>直播间点赞评论</p>
                        <p>为直播间点赞评论，获得更多展现曝光，上热门</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_button">
                        <div class="button"><img src="~/Content/images/index/zxbtn.png" alt="" /></div>
                    </div>
                </div>
            </div>

        </div>

        <div class="live">
            <h3 id="miao2">—— 视频智能增值服务  ——</h3>
            <div class="live_left">
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/Videoicn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>视频点赞服务</p>
                        <p>为你的视频点赞助力，视频点赞轻松过万不是梦</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/commentsicn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>视频评论服务</p>
                        <p>打造热卖精品评论，获得更多展现和曝光</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_img"><img src="~/Content/images/index/commenticn.png" alt="" /></div>
                    <div class="live_left_div_text">
                        <p>视频子评服务</p>
                        <p>子评，追评，在你想要的评论下进行盖楼评论， 打造出单条评论的火爆</p>
                    </div>
                </div>
                <div class="live_left_div">
                    <div class="live_left_div_button">
                        <div class="button"><img src="~/Content/images/index/zxbtn.png" alt="" /></div>
                    </div>
                </div>
            </div>
            <div class="live_right">
                <img src="~/Content/images/index/Videoillustration.png" alt="" />
            </div>
        </div>

        <div class="choose">
            <h3 id="miao3">—— 为什么选择我们？ ——</h3>
            <div class="chooseimg">
                <img src="~/Content/images/index/choose.png" />
            </div>
        </div>

        <div class="Live_sentiment">
            <h3 id="miao5">—— 直播人气实时效果 ——</h3>
            <div class="Live_sentiment_img">
                <img src="~/Content/images/index/Live_sentiment.png" alt="" />
            </div>
        </div>

        <div class="feedback">
            <h3>—— 用户需求反馈 ——</h3>
            <div class="feedbackimg">
                <img src="~/Content/images/index/feedback.png" alt="" />
            </div>
        </div>

        <div class="bottomat">
            <div class="bottomatimg">
                <img src="~/Content/images/index/bottomat.png" alt="" />
            </div>
            <div class="bottomatcode">
                <img src="~/Content/images/index/xiaoa.jpg" alt="" />
            </div>
        </div>

        <div class="floor">
            <div class="floor_div" style="padding-top: 15px;">
                <!--<div class="floor_div_img">
                    <img src="~/Content/images/index/logo.png" alt="" />
                </div>-->
                <div class="floor_div_text">
                    <div>关于我们</div>
                    <div>
                        <span>功能特性</span>
                        <span>产品优势</span>
                    </div>
                </div>
            </div>
            @*<div class="floor_text">小艾部落（衡水）科技有限公司 All Rights Reserved 版权所有</div>*@
        </div>

    </div>

    <!--弹窗-->
    <div class="dialog">
        <div class="changePwd">
            <div class="title">
                <span class="right closeBtn"></span>
            </div>
        </div>
    </div>

    @*<script type="text/javascript" src="siteplugins/js/jquery.js"></script>*@
    <script src="~/Content/js/jquery.min.js"></script>
    <script type="text/javascript">
        $(".button").click(function () {
            $(".dialog").show()
        })
        $(".closeBtn").click(function () {
            $(".dialog").hide()
        })        
    </script>
</body>
</html>
